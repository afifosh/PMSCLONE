{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0.2",
    "akaunting/laravel-money": "^4.0",
    "barryvdh/laravel-debugbar": "^3.7",
    "ddeboer/imap": "^1.17",
    "firebase/php-jwt": "^6.4",
    "genealabs/laravel-pivot-events": "^10.0",
    "google/apiclient": "^2.13",
    "guzzlehttp/guzzle": "^7.2",
    "imanghafoori/laravel-password-history": "^1.0",
    "jdavidbakr/mail-tracker": "^7.6",
    "jenssegers/agent": "^2.6",
    "kub-at/php-simple-html-dom-parser": "^1.9",
    "lab404/laravel-impersonate": "^1.7.4",
    "laravel/fortify": "^1.16",
    "laravel/framework": "^10.0",
    "laravel/pint": "^1.4",
    "laravel/sanctum": "^3.2",
    "laravel/telescope": "^4.14",
    "laravel/tinker": "^2.7",
    "laravelcollective/html": "^6.4",
    "laravolt/avatar": "^5.0",
    "league/oauth2-google": "^4.0",
    "maatwebsite/excel": "^3.1",
    "mavinoo/laravel-batch": "^2.3",
    "microsoft/microsoft-graph": "^1.97",
    "nwidart/laravel-modules": "^10.0",
    "owen-it/laravel-auditing": "^13.5",
    "pelago/emogrifier": "^7.0",
    "pion/laravel-chunk-upload": "^1.5",
    "plank/laravel-mediable": "^5.9",
    "plank/laravel-metable": "^5.4",
    "predis/predis": "^2.1",
    "propaganistas/laravel-phone": "^5.0",
    "ralouphie/mimey": "^1.0",
    "rappasoft/laravel-authentication-log": "^3.0",
    "spatie/laravel-activitylog": "^4.7",
    "spatie/laravel-permission": "^5.10",
    "spatie/once": "^3.1",
    "stephenlake/laravel-approval": "^1.1",
    "torann/geoip": "^3.0",
    "webpatser/laravel-countries": "^1.5",
    "willdurand/email-reply-parser": "^2.10",
    "yajra/laravel-datatables": "^10.1",
    "yajra/laravel-datatables-buttons": "^10.0",
    "yajra/laravel-datatables-editor": "^1.25",
    "yajra/laravel-datatables-fractal": "^10.0",
    "yajra/laravel-datatables-oracle": "^10.4",
    "zbateson/mail-mime-parser": "^2.4"
  },
  "require-dev": {
    "beyondcode/laravel-query-detector": "^1.7",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/Helpers.php",
      "app/Helpers/helper.php",
      "app/Helpers/onlyofficeHelpers.php",
      "app/Support/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
